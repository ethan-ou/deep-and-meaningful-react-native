{
  "expo": {
    "name": "Deep and Meaningful",
    "slug": "deep-and-meaningful",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon-dark.png",
    "backgroundColor": "#333333",
    "splash": {
      "image": "./src/assets/splash-dark.png",
      "resizeMode": "cover",
      "backgroundColor": "#333333"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "src/assets/**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.ethanou.deepandmeaningful",
      "buildNumber": "1.0.0",
      "supportsTablet": true
    },
    "android": {
      "package": "com.ethanou.deepandmeaningful",
      "versionCode": 1
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "js",
        "jsx",
        "svg",
        "ts",
        "tsx"
      ]
    },
    "userInterfaceStyle": "automatic"
  }
}
